                    <script type="text/javascript">
                        try {
                            var chartDataSource = { @Html.Raw(Model.GetCandles) };
                            require(['jquery', 'iguanachart'], function ($) {
                                $('#iChart').on("iguanaChartEvents", function (e, name, data) {
                                    if (name == "drawComplete") {
                                        $("[name='SelectInstrument']").removeClass("active");
                                    }
                                }).iguanaChart({
                                    lib_path: "/dist",
                                    ticker: "@Model.MarketName",
                                    chartOptions: {
                                        minHeight: $('#iChart').height(),
                                        futureAmount: 1,
                                        floatingLegend: true,
                                        floatingLegendFontSize: 13,
                                        floatingLegendTextColor: "#444444",
                                        floatingLegendBorderColor: "rgba(82,175,201,1)",
                                        floatingLegendBackground: "rgba(82,175,201,.6)",
                                        uiTools: { top: true },
                                    },
                                    dataSource: $.extend(true, {}, chartDataSource)
                                });

                                $(document).on("click", "[name='SelectInstrument']", function () {
                                    $("#iChart").iguanaChart("toolStart", $(this).data("instrument"));
                                    $("[name='SelectInstrument']").removeClass("active");
                                    $(this).addClass("active");
                                });

                            @if (Session["DARK"] != null)
                            {
                                @:$("#iChart").iguanaChart("setTheme", "Dark");
                            }
                            else {
                                @:$("#iChart").iguanaChart("setTheme", "White");
                            }

                                var pair = "@Model.MarketName";
                                var timeF = "@Model.TimeFrameValue";
                                var pairSplit = "@Model.pairSplit"
                                var utcTime = "@Model.timeOffset"

                                setInterval(function () {

                                    $.get("@Url.Action("GetCandlesData", "Dashboard")", { pair: pair, timeFrame: timeF, utcTime: utcTime }, function (data) {

                                        var tm = data.obj.Ticker, vl = data.obj.Volume, preHloc = [], hloc = [];
                                        var point = $("#iChart").data("iguanaChart").getLastPoint();

                                        preHloc[0] = data.obj.High;
                                        preHloc[1] = data.obj.Low;
                                        preHloc[2] = data.obj.Open;
                                        preHloc[3] = data.obj.Close;

                                        hloc[0] = Math.max.apply(null, preHloc);
                                        hloc[1] = Math.min.apply(null, preHloc);
                                        hloc[2] = preHloc[2];
                                        hloc[3] = preHloc[3];

                                        var result = {
                                            "hloc": {},
                                            "vl": {},
                                            "xSeries": {}
                                        };

                                        result["hloc"][Object.keys(point.xSeries)[0]] = [hloc];
                                        result["vl"][Object.keys(point.xSeries)[0]] = [vl];
                                        result["xSeries"][Object.keys(point.xSeries)[0]] = [tm];
                                        $("#iChart").iguanaChart("addPoint", result);
                                    });
                                }, 2500);

                                var isLogin = "@Model.IsLogin";
                                if (isLogin === 'True') {
                                    setInterval(function () {
                                        var offset = new Date().getTimezoneOffset();
                                        var pair = "@Model.MarketName";
                                        var point = $("#iChart").data("iguanaChart").getLastPoint();
                                        var ticker = point.xSeries['LKOH'][0];
                                        $.get("@Url.Action("GetTradeOnChart", "Dashboard")", { timeOffset: offset, pair: pair, point: ticker }, function (dt) {
                                            $("#iChart").on("iguanaChartEvents", function (e, name, data) {
                                                if (dt.res.length != 0) {
                                                    $("#iChart").data("iguanaChart").removeTransactions();
                                                    $("#iChart").data("iguanaChart").addTransactions(dt.res, 2);
                                                }
                                                else {
                                                    $("#iChart").data("iguanaChart").removeTransactions();
                                                }
                                            });
                                        });
                                    }, 3000)
                                }
                            });
                        }
                        catch (ex) {
                            alert("Error")
                        }
                    </script>
                </div>
