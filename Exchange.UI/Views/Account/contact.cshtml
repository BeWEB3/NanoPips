    <script src="~/Assets2/js/index.js" type="text/javascript"></script>
    <script src="~/Assets2/js/owl.carousel.min.js"></script>

    <script>

        $("#submit").on('click', function () {
            var name    = $("#name").val();
            var email   = $("#email").val();
            var phone   = $("#phone").val();
            var subject = $("#subject").val();
            var message = $("#Message").val();
            if (name === "") { alert("Name field should not empty"); }
            else if (email === "") { alert("Name field should not empty"); }
            else if (phone === "") { alert("Phone Number field should not empty"); }
            else if (subject === "") { alert("Subject field should not empty"); }
            else if (message === "") { alert("Message field should not empty"); }
            else {
                $.get("/Account/SendEmail?name=" + name + "&email=" + email + "&phone=" + phone + "&subject=" + subject + "&message=" + message, function (obj) {
                    if (obj === true) {
                        alert("Email Sent, we will get back to you soon. ");
                        location.reload(true);
                    }
                    else {
                        alert("An Error Occured, please try again. ");
                    }
                });
            }
        });
    </script>

