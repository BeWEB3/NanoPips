    <script>
        $(function () {
            $('[data-toggle="tooltip"]').tooltip();
        });
    </script>
    <script>
        $(function () {
            $(".showSideIcon").click(function () {
                $(".sidebar-menu").css("display", "block");
            });
        });
    </script>
    <script>
        $(function () {
            $(".dt").DataTable({
                aaSorting: [[0, 'desc']]
            });
            var getNotifications = setInterval(function () {
            $.ajax({
                url: "@Url.Action("_notification","Account")",
                type: "POST",
                success: function (html) {
                    $("#not").html(html);
                },
                error: function () {
                    clearInterval(getNotifications);
                }
            })
        }, 1000 * 60);
        })
    </script>
    <script>
        $(function () {
            $(".showNavMob").click(function () {
                var x = document.getElementById("mobile-containerId");
                if (x.style.display === "none") {
                    x.style.display = "block";
                }
                else {
                    x.style.display = "none";
                }
            });
        });
    </script>

    <script>
            $(function () {

            $(".use-loader").click(function () {
                $.blockUI();
            });
            var msg = "@TempData["msg"]";
            if (msg != "") {
                bootbox.alert(msg);

            }
            var msg1 = "@ViewBag.msg";
            if (msg1 != "") {
                bootbox.alert(msg1);

            }
            var twoFA = '@ViewBag.twoFA';
            if (twoFA == "False") {
                bootbox.dialog({
                    title: "<h3> <i class='fa fa-warning'></i> <span> Please set up two-factor authentication</span></h3>",
                    message: "Please seriously consider enabling two-factor authentication. Enabling 2FA adds an extra layer of security to your account and protects you against the most common password crack and social engineering hacks.<br>To increase your security now, please proceed to the <a class='' href='/Dashboard/TwoFactorAuthentication' style='color:#0094ff' > Enable 2FA</a> and enable two-factor authentication. <br/> <strong>NOTE: You cannot withdraw funds without 2FA</strong>",
                    buttons: {
                        ok: {
                            label:"Ok, I understand the risk"
                        }
                    }
                })
            }
        });

    </script>
